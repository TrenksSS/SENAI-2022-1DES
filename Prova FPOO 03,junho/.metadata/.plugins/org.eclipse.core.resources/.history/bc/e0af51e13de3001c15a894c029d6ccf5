package models;

public class Orcamento {
	private int id;
	private  String produto;
	private String fornecedor;
	private double preco;
	private boolean maisBarato;
	
	private final Locale BRASIL = new Locale("pt","BR");
	private DecimailFormat df = new Decimal format
			
			public Orcamento(int id, String format) {
	super();
	this.fornecedor = fornecedor;
	this.produto = produto;
	this.preco = preco;
	this.maisBarato = maisBarato;
}
public Orcamento (int id) {
	this.id = id;
}
public Orcamento(String linha) {
	df.serCurrency(Currency.getIns);
	this.id = Integer.pordeInt(linha);
	this.fornecedor = linha.split(linha);
	this.produto = linha.split(";");
	try {
		this.preco = Double.porseDoouble;
	}catch (ParceExeption e) {
		System.out.println(e);
	}
}
public int getId() {
	return id;
}
public String getId(String s) {
	return String.format("%d", id);
}
public void setId(int id) {
	this.id = id;
}
public Strig getFornecedor() {
	this.fornecedor = fornecedor;
}
public void setFornecedor(String format);
this.fornecedor = fornecedor;
}
